/* component/zyslider.wxss */

.container {
  /*height: 100%;*/
  width: 100%;
  position: relative;
  height: 16px;
  border-radius: 16px;
  margin-bottom: 62rpx;
  padding-top: 25rpx;
}

.slider-body {
  height: 12rpx;
  background-color: #ddd;
  position: relative;
}

.body {
  bottom: 60rpx;
  background-color: #3668D0;
  z-index: 0.3;
}

.left {
  bottom: 48rpx;
  z-index: 0.1;
}

.right {
  z-index: 0.2;
  bottom: 72rpx;
}


.min {
  position: relative;
  top: 18rpx;
  /*left: 100rpx;*/
}

.max {
  position: relative;
  bottom: 23rpx;
  /*left: 600rpx;*/
}

.hide {
  display: none;
}

.slider-item {
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  z-index: 2;
  background-size: contain;
  background-image: url(data:image/png;base64,)
}
.show-money {
  position: absolute;
  top: 0;
  color: rgba(41, 77, 126, 1);
  font-size: 26rpx;
  width: 200rpx;
  margin-left: 50%;
  left: -100rpx;
  text-align: center;
}